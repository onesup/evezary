<div class="row-fluid">
  <!-- block -->
  <div class="block">
    <div class="navbar navbar-inner block-header">
      <div class="muted pull-left">조사 현황</div>
    </div>
    <div class="block-content collapse in">
      <div class="span12">
        <table cellpadding="0" cellspacing="0" border="0" class="table table-striped table-bordered" id="data-table">
          <thead>
            <tr>
              <th>번호</th>
              <th>참여자</th>
              <th>득표</th>
            </tr>
          </thead>
          <tbody>
            <% @surveys.each do |survey| %>
              <tr class="">
                <td>
                  <%= link_to(survey.id, nil, title: survey.id) %>
                </td>
                <td>
                  <%= link_to survey.title, admin_survey_path(survey) %>
                </td>
                <td class="updated_at">
                  <p><%= survey.users.count %></p>
                </td>
              </tr>
            <% end %>
          </tbody>
        </table>
      </div>
    </div>
  </div>
  <!-- /block -->
</div>
